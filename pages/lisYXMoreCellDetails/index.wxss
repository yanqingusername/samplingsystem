page{
  background: #F5F5F5;
}

.gps_container{
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.child-section {
  background-color: #f5f5f5;
  height: 100%;
}

.main {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;

}

.s_center_t_item {
  font-size: 28rpx;
  color: #999999;
}


.select-item {
  width: 74%;
}


.input-item {
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

  input {
    width: 100%;
    height: 80rpx;
    background: #ffffff;
    border-radius: 12rpx;
    border: none;
    outline: none;
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #333333;
    /* padding-left: 20rpx; */
  }






    

  

  
.empty_view {
  height: 50rpx;
  width: 100%;
}


.search-list-role-from {
  border: 0 none;
  width: 100%;
  padding: 0rpx 30rpx;
  margin-top: 30rpx;
  border-radius: 20rpx;
}

  .search-list-role-from-a {
    width: 100%;
    display: flex;
    flex-direction: column;
    padding: 10rpx 20rpx;
    background: #ffffff;
    border-radius: 20rpx;
  }

    .search-list-role-from-b {
      padding-top: 10rpx;
      font-size: 32rpx;
    }

    .search-list-role-from-c {
      width: 100%;
      display: flex;
      justify-content: space-between;
      align-items: center;
      background: #ffffff;
      padding: 0rpx 0rpx;
      border-bottom: 1px solid #dddddd;
    }

   /**
   *
   */
   .search-list-role {
    border: 0 none;
    background-color: #f5f5f5;
    width: 100%;
    padding: 0px 30rpx;
  }

  .search-result-view {
    display: flex;
    padding: 30rpx 30rpx;
background-color: #FFFFFF;
border-radius: 10rpx;
margin-top: 30rpx;
flex-direction: column;

   }

   .dis_setting {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 40rpx 30rpx;
  }
  .search-result-view-1 {
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #666666;
  }

  .search-result-view-2 {
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #333333;
    max-width: 200rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
  }
  

  .search-result-view-3 {
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #999999;
  }

  .icon_arrow{
    width: 20rpx;
    height: 24rpx;
    margin-left: 6rpx;
  }
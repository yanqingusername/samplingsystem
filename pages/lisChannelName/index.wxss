page {
  background: #F5F5F5;
}

.child-section {
  background-color: #f5f5f5;
  height: 100%;
}

.main {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.search-result-view {
  display: flex;
  padding: 20rpx 30rpx;
  background-color: #FFFFFF;
  border-radius: 10rpx;
  margin-top: 30rpx;
}

.search-result-view-1 {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;

}

.search-result-view-2 {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
  margin-left: 10rpx;

}

.search-result-view-3 {
  display: flex;
  align-items: center;
  color: #999999;
}

.icon_arrow{
  width: 20rpx;
  height: 20rpx;
}

.dis_setting {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 40rpx 30rpx;
}

.dis_setting_view {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10px 30rpx 10px 30rpx;
  background:#FFFFFF;
}

.search-list-role {
  border: 0 none;
  background-color: #F5F5F5;
  width: 100%;
  padding: 0px 30rpx;
}

.s_center_t_item {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #307FF5;
}

.empty_view {
  height: 50rpx;
  width: 100%;
}

.s_center_t_empty {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  padding: 80rpx;
  font-size: 28rpx;
  color: #333333;
}

.search
{
  display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    width: 85%;
    height: 70rpx;
    border-radius: 20rpx;
    position: relative;
}

/* 搜索框提示文字样式 */
.search input
{
  font-size:27rpx;
  font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  /* padding-left:30rpx;  */
}
#search_checked{
  width:100%;
  text-align: initial;
  background-color: #F5F5F5;
  height: 70rpx;
  /* padding-top: 12rpx; */
  padding-left: 16rpx;
  /* padding-right: 150rpx; */
  border-radius: 10rpx;
}
.weui-icon-clear{
  position: absolute;
  right: 0rpx;
  top: 6rpx;
  z-index: 100;
  padding: 10rpx 15rpx;
}